<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mti.apppanfleto">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PanfletoPrincipal" />
        <activity
            android:name=".ProductosActivity"
            android:label="@string/app_name"
            android:parentActivityName=".PanfletoPrincipal"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PanfletoPrincipal" />
        </activity>
        <activity
            android:name=".SplashCrimActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalendarioActivity"
            android:label="@string/app_name"
            android:parentActivityName=".PanfletoPrincipal"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PanfletoPrincipal" />
        </activity>
        <activity
            android:name=".ProductActivity"
            android:label="@string/app_name"
            android:parentActivityName=".PanfletoPrincipal"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PanfletoPrincipal" />
        </activity>
    </application>

</manifest>